{
  "name": "dotascope",
  "version": "0.0.2",
  "description": "Dota 2 Counterpicker",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "npm run build && node src/server/server.js",
    "dev": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "test": "jest",
    "seed": "node src/server/utils/seedDatabase.js",
    "populate": "node src/server/utils/populateDatabase.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-jest": "^6.0.1",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "eslint": "^1.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.2",
    "html-webpack-plugin": "^1.6.1",
    "react-hot-loader": "^1.2.8",
    "transfer-webpack-plugin": "^0.1.4",
    "react-addons-test-utils": "^0.14.2",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "cheerio": "^0.19.0",
    "classnames": "^2.2.1",
    "express": "^4.13.3",
    "fbjs": "^0.4.0",
    "jquery": "^2.1.4",
    "mongoose": "^4.2.7",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "request": "^2.67.0"
  }
}
